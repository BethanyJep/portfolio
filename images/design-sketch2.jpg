  <!-- Process diagram -->
  <g fill="none" stroke="#333333" stroke-width="2">
    <rect x="150" y="450" width="100" height="60" rx="10"/>
    <rect x="350" y="450" width="100" height="60" rx="10"/>
    <rect x="550" y="450" width="100" height="60" rx="10"/>
    <path d="M250,480 L350,480" stroke-dasharray="5,5"/>
    <path d="M450,480 L550,480" stroke-dasharray="5,5"/>
    <polygon points="340,475 350,480 340,485"/>
    <polygon points="540,475 550,480 540,485"/>
  </g>
  
  <rect x="150" y="450" width="100" height="60" rx="10" fill="#ffd6a5" opacity="0.7"/>
  <rect x="350" y="450" width="100" height="60" rx="10" fill="#ffd6a5" opacity="0.7"/>
  <rect x="550" y="450" width="100" height="60" rx="10" fill="#ffd6a5" opacity="0.7"/>
  
  <text x="200" y="485" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle" fill="#333333">DISCOVER</text>
  <text x="400" y="485" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle" fill="#333333">DEFINE</text>
  <text x="600" y="485" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle" fill="#333333">DESIGN</text>
  
