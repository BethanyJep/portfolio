  <rect x="450" y="270" width="250" height="120" rx="10" fill="#b4f2c8" opacity="0.7"/>
  <rect x="470" y="290" width="210" height="20" rx="5" fill="#333333" opacity="0.1"/>
  <rect x="470" y="320" width="160" height="20" rx="5" fill="#333333" opacity="0.1"/>
  <rect x="470" y="350" width="190" height="20" rx="5" fill="#333333" opacity="0.1"/>
