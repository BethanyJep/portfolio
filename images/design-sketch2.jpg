  <circle cx="130" cy="610" r="5" fill="#333333"/>
  <text x="150" y="615" font-family="Arial" font-size="14" fill="#333333">Users need simpler onboarding process</text>
  
  <circle cx="130" cy="640" r="5" fill="#333333"/>
  <text x="150" y="645" font-family="Arial" font-size="14" fill="#333333">Navigation confusion on mobile devices</text>
  
  <circle cx="130" cy="670" r="5" fill="#333333"/>
  <text x="150" y="675" font-family="Arial" font-size="14" fill="#333333">Feature discovery is a major pain point</text>
  
  <!-- Hand drawn sketch -->
  <g stroke="#333333" stroke-width="1" fill="none">
    <rect x="450" y="570" width="200" height="90" rx="5"/>
    <line x1="450" y1="590" x2="650" y2="590"/>
    <line x1="500" y1="570" x2="500" y2="660"/>
    <line x1="550" y1="570" x2="550" y2="660"/>
    <line x1="600" y1="570" x2="600" y2="660"/>
    <circle cx="500" cy="610" r="10"/>
    <circle cx="550" cy="630" r="10"/>
    <circle cx="600" cy="600" r="10"/>
  </g>
  
  <!-- Footer -->
  <line x1="100" y1="720" x2="700" y2="720" stroke="#333333" stroke-width="2"/>
  <text x="120" y="750" font-family="Arial" font-size="14" fill="#666666">Workshop Date: May 15, 2025</text>
  <text x="400" y="750" font-family="Arial" font-size="14" fill="#666666">Facilitator: Alex Johnson</text>
  <text x="650" y="750" font-family="Arial" font-size="14" text-anchor="end" fill="#666666">Page 2/4</text>
