  <text x="400" y="120" font-family="Arial" font-size="28" font-weight="bold" text-anchor="middle" fill="#333333">UX WORKSHOP NOTES</text>
  
  <!-- Hand-drawn elements -->
  <g stroke="#333333" stroke-width="2" fill="none">
    <path d="M100,200 C150,190 170,230 200,220"/>
    <path d="M220,220 C250,210 270,240 300,230"/>
    <path d="M320,230 C350,220 370,250 400,240"/>
  </g>
  
  <!-- Main content boxes -->
  <rect x="100" y="270" width="250" height="120" rx="10" fill="#ffcbdb" opacity="0.7"/>
  <rect x="120" y="290" width="210" height="20" rx="5" fill="#333333" opacity="0.1"/>
  <rect x="120" y="320" width="160" height="20" rx="5" fill="#333333" opacity="0.1"/>
  <rect x="120" y="350" width="190" height="20" rx="5" fill="#333333" opacity="0.1"/>
